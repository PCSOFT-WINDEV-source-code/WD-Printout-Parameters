   -
     name : LoadPrintoutParameter
     internal_properties : CAAAAAgAAACqCjf/o+GNw7xfDELjKFeMAWQfAVM0BAUuDuzk721Gl/CQnJC93CSopLqdUuoFC8mqI9WcjZeB9KF0dgmcPmJ0x/OIdSQNeYVEUxU51z/3TwA7TIgEPyA/38wn79Qdmfw2/ZYGC76OSVkkMtkAPqnQW8ldDLaDgiksYcCSab1XzrFjG/Ge3wRxGXbYFBFxLGab2vualR0zWxKxty80e7OFO4QkvFRN
     procedure_id : 1653195687487308519
     type_code : 14
     code : |1-
      // Summary: This procedure is used to update the information with the default settings of the printer
      
      PROCEDURE LoadPrintoutParameter()
      
      sPrinterName is string // Name of the selected printer
      nSubscript is int // Subscript in the combo box that corresponds to the default parameter
      
      // Check the printer combo box
      IF COMBO_PRINTERS = -1 THEN
      	// No printer is selected
      	RETURN
      END
      
      sPrinterName = COMBO_PRINTERS[COMBO_PRINTERS]
      
      // Select the current printer
      iConfigure(sPrinterName)
      
      // Collated Copies
      CBOX_COLLATE = iParameter(iCollate)
      
      // Color or Black/White?
      IF iParameter(iColor) = iColor_Color THEN
      	// Color
      	RADIO_COLOR = 1
      ELSE
      	// Black and White
      	RADIO_COLOR = 2	
      END
      
      // Orientation of paper
      IF iParameter(iOrientation) = iOrientation_Landscape THEN
      	RADIO_ORIENTATION = 2
      ELSE
      	RADIO_ORIENTATION = 1
      END
      
      // Duplex
      SWITCH iParameter(iDuplex)
      	CASE iDuplex_Simple
      		RADIO_DOUBLE_SIDED = 1
      	CASE iDuplex_Vertical
      		RADIO_DOUBLE_SIDED = 2
      	CASE iDuplex_Horizontal
      		RADIO_DOUBLE_SIDED = 3
      END
      
      // TrueType Font:
      nSubscript = ListSeek(COMBO_TRUETYPE, iParameter(iTrueType))
      IF nSubscript <> -1 THEN
      	ListSelectPlus(COMBO_TRUETYPE, nSubscript)
      END
      
      // Scale
      EDT_SCALE = iParameter(iScale)
      SPIN_Scale..Value = EDT_SCALE
      
      // Number of copies
      EDT_NB_COPIES = iParameter(iCopies)
      SPIN_Copies..Value = EDT_NB_COPIES
      
      
      // Save the parameters in Windows
      CBOX_SAVE = iParameter(iPrinterDefault)
      
      // Changes the image of collated copies (if necessary)
      ExecuteProcess(CBOX_COLLATE, trtChange)
      
      HourGlass(False)
     type : 458752
   -
     name : SavePrintoutParameter
     procedure_id : 1653195687487374055
     type_code : 14
     code : |1-
      // Summary: This procedure is used to save the selected settings
      
      PROCEDURE SavePrintoutParameter()
      
      sPrinterName is string
      
      // Check the selected printer
      IF COMBO_PRINTERS = -1 THEN
      	Error(<§$0003§>)
      	RETURN
      END
      
      // Retrieve the selected printer
      sPrinterName = COMBO_PRINTERS[COMBO_PRINTERS]
      
      // Configure the selected printer
      iConfigure(sPrinterName)
      
      // Display a hourglass if the settings must be saved as default settings
      // because the assignment can take quite a long time
      IF CBOX_SAVE THEN
      	
      	Info(<§$0000§>,...
      	<§$0001§>,...
      	<§$0002§>)
      	
      	HourGlass(True)
      	
      END
      
      // Save the parameter for collating copies
      iParameter(iCollate, CBOX_COLLATE)
      
      // Save the management of TrueType fonts
      iParameter(iTrueType, COMBO_TRUETYPE[COMBO_TRUETYPE])
      
      // Save the scale
      iParameter(iScale, EDT_SCALE)
      
      // Save the number of copies
      iParameter(iCopies, EDT_NB_COPIES)
      
      // Save the orientation
      SWITCH RADIO_ORIENTATION
      	CASE 1 // Portrait
      		iParameter(iOrientation, iOrientation_Portrait)
      	CASE 2 // Landscape
      		iParameter(iOrientation, iOrientation_Landscape)
      	OTHER CASE
      		// no action
      END
      
      // Save the duplex printout
      SWITCH RADIO_DOUBLE_SIDED
      	CASE 1 // No duplex printing
      		iParameter(iDuplex, iDuplex_Simple)
      	CASE 2 // Vertical-Duplex Printing
      		iParameter(iDuplex, iDuplex_Vertical)
      	CASE 3 // Horizontal-Duplex Printing
      		iParameter(iDuplex, iDuplex_Horizontal)
      	OTHER CASE 
      		// No action
      END
      
      // Save the color configuration
      SWITCH RADIO_COLOR
      	CASE 1 // Color
      		iParameter(iColor, iColor_Color)
      	CASE 2 // Monochrome
      		iParameter(iColor, iColor_Mono)
      	OTHER CASE 
      		// No action
      END
      
      // Save the "default" parameter
      iParameter(iPrinterDefault, CBOX_SAVE)
      
      // Remove the hourglass (if CBOX_SAVE is checked)
      HourGlass(False)
     type : 458752
